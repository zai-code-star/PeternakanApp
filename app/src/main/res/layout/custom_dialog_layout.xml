<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="328dp"
    android:background="@color/white"
    android:layout_height="350dp">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="328dp"
        android:layout_height="350dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dialog" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yakin Ingin Menghapus ?"
        android:textColor="@color/primary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toTopOf="@+id/imageView18"
        app:layout_constraintVertical_bias="0.048" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:layout_constraintVertical_bias="0.25"
        app:srcCompat="@drawable/vektor_dialog" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonbatal"
        android:layout_width="127dp"
        android:layout_height="50dp"
        android:onClick="onButtonJualClick"
        android:padding="12dp"
        android:text="Batalkan"
        android:textColor="@color/primary"
        android:textSize="14sp"
        app:backgroundTint="#FFBB00"
        app:cornerRadius="100dp"
        app:icon="@null"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:strokeColor="@android:color/transparent"
        app:strokeWidth="0dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonhapus"
        android:layout_width="127dp"
        android:layout_height="50dp"
        android:onClick="onButtonLaporClick"
        android:paddingStart="12dp"
        android:paddingEnd="24dp"
        android:text="Hapus"
        android:textColor="@color/kuning"
        app:backgroundTint="@android:color/transparent"
        app:cornerRadius="100dp"
        app:icon="@null"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/buttonbatal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.354"
        app:layout_constraintStart_toEndOf="@+id/buttonbatal"
        app:layout_constraintTop_toTopOf="@+id/buttonbatal"
        app:strokeColor="#FFBB00"
        app:strokeWidth="0.75dp" />


</androidx.constraintlayout.widget.ConstraintLayout>